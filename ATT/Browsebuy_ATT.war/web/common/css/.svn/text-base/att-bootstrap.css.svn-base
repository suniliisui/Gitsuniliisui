html {
    font-size: 62.5%;
    -webkit-text-size-adjust: 62.5%;
    -ms-text-size-adjust: 62.5%;
}
body {
    font-family: 'Helvetica Neue LT Std', 'Helvetica Neue', 'Helvetica'; 
    font-size: 14px;
    font-size: 1.4rem;
    background-color: #f2f2f2;
    margin: 0;
}
a {
    text-decoration: none;
    -webkit-transition: all .3s ease-out;
    -moz-transition: all .3s ease-out;
    transition: all .3s ease-out;

}
[class^="icon-"], [class*=" icon-"] {
    font-family: 'atticons';
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    height: auto;
    width:auto;
    background: none;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.att-radio, .att-checkbox {
    height: 24px;
    width: 24px;
    background-color: #f8f8f8;
    border: 1px solid #d5d5d5;
    border-radius: 24px;
    text-align: center;
    display: inline-block;
    line-height: 24px;
    cursor: pointer;
    vertical-align: middle;
    -webkit-transition: all .2s ease-out;
    -moz-transition: all .2s ease-out;
    transition: all .2s ease-out;
}
.att-checkbox {
    border-radius: 3px;
}
.att-radio:hover, .att-checkbox:hover,
.att-radio:focus, .att-checkbox:focus {
    border-color: #bbb;
}
.att-radio-indicator, .att-checkbox-indicator {
    position: absolute;
    width: 6px;
    height: 6px;
    background: #0579b4;
    position: relative;
    border-radius: 6px;
    left: 50%;
    top: 50%;
    margin: -3px;
    opacity: 0;
    -ms-filter: "alpha(opacity=0)";
    -webkit-transition: all .2s ease-out;
    -moz-transition: all .2s ease-out;
    transition: all .2s ease-out;
}
.att-checkbox-indicator {
    width: 24px;
    height: 24px;
    left: 0;
    top: 0;
    margin: 0;
    background: 0;
}
.att-checkbox-indicator:after {
    content: '\e034';
    position: absolute;
    display: block;
    width: 24px;
    left: 0;
    color: #157ab2;
    text-align: center;
    font-family: atticons;
    font-size: 20px;
}
.att-radio.on .att-radio-indicator, .att-checkbox.on .att-checkbox-indicator {
    opacity: 1;
    -ms-filter: "alpha(opacity=100)"}
.att-radio input, .att-checkbox input {
    position: absolute;
    left: -9999px;
}
.att-switch {
    display: inline-block;
    vertical-align: middle;
    width: 51px;
    height: 21px;
    line-height: 21px;
    background-color: #ddd;
    border-radius: 21px;
    position: relative;
    overflow: hidden;
    border: 1px solid transparent;
    -webkit-user-select: none;
    -moz-user-select: none;
    user-select: none;
}
.ie .att-switch {
    border: 0;
}
.att-switch-content {
    position: absolute;
    width: 81px;
    font-size: 0;
    top: 0;
    left: -30px;
    overflow: hidden;
    -webkit-transition: left .2s ease-in-out 0;
    -moz-transition: left .2s ease-in-out 0;
    transition: left .2s ease-in-out 0;
}
.att-switch-content.dragging {
    -webkit-transition: left 0;
    -moz-transition: left 0;
    transition: left 0;
}
.att-switch.on .att-switch-content {
    left: 0;
}
.att-switch-onText, 
.att-switch-thumb, 
.att-switch-offText {
    display: inline-block;
    vertical-align: middle;
    font-size: 12px;
    font-size: 1.2rem;
    width: 30px;
    height: 21px;
    text-align: center;
    cursor: default;
    color: #6f6f6f;
    margin: 0;
    -webkit-transition: background-color .2s ease-out, color .2s ease-out;
    -moz-transition: background-color .2s ease-out, color .2s ease-out;
    transition: background-color .2s ease-out, color .2s ease-out;
}
.lt-ie9 .att-switch-onText, 
.lt-ie9 .att-switch-thumb, 
.lt-ie9 .att-switch-offText {
    margin-top: -1px;
}
.att-switch-thumb {
    width: 21px;
    border-radius: 100%;
    background: #777;
    position: relative;
}
.att-switch.on .att-switch-thumb {
    background: #1e7cb0;
}
.att-switch-thumb:after, 
.att-switch.on .att-switch-thumb:after {
    font-family: atticons;
    content: '\e034';
    color: #fff;
    font-size: 13px;
    font-size: 1.3rem;
    line-height: 21px;
    line-height: 2.1rem;
}
.att-switch.on .att-switch-thumb:after {
    content: '\e0db'}
.att-switch-alt .att-switch-thumb:after {
    display: none;
}
.att-switch:hover .att-switch-thumb, 
.att-switch:active .att-switch-thumb {
    background: #269edc;
}
.att-switch:hover .att-switch-onText, 
.att-switch:hover .att-switch-offText {
    color: #bbb;
}
.att-switch .att-switch-onText, 
.att-switch .att-switch-offText {
    visibility: hidden;
}
.att-switch-alt .att-switch-onText, 
.att-switch-alt .att-switch-offText {
    visibility: visible;
}
.att-switch-onText {
    text-indent: 4px;
}
.att-switch-offText {
    text-indent: -4px;
}
.att-switch input {
    visibility: hidden;
    position: absolute;
    left: -100px;
}
.tabs {
    display: inline-block;
    list-style: none;
    margin: 0;
    padding: 0;
    font-size: 0;
}
.tabs>li {
    padding: 0;
    margin: 0;
    display: inline-block;
}
.tabs>li a {
    height: 30px;
    width: auto;
    margin: 0 0 0 -1px;
    padding: 0 30px;
    background: #f2f2f2;
    border: 1px solid #e7e6e6;
    text-align: center;
    font-size: 11px;
    font-size: 1.1rem;
    color: #6f6f6f;
    text-transform: uppercase;
    line-height: 31px;
    cursor: pointer;
    display: inline-block;
}
.tabs>li.first a, 
.tabs>li:first-child a {
    border-radius: 4px 0 0 4px;
    margin-left: 0;
}
.tabs>li.last a, 
.tabs>li:last-child a {
    border-radius: 0 4px 4px 0;
}
.tabs>li a:hover {
    background: #e5e5e5;
    -webkit-transition: all .3s ease-out;
    -moz-transition: all .3s ease-out;
    transition: all .3s ease-out;
}
.tabs>li.active a {
    background: #f8f8f8;
    color: #067ab4;
}
.tabs-alt>li a {
    padding: 0 15px;
}
.tabs-icons>li a, 
.tabs-icons>li.active a, 
.tabs-icons>li a:hover {
    padding: 0;
    border: 0;
    background: 0;
    font-size: 28px;
    line-height: 36px;
    margin-right: 10px;
}
.tabs-icons>li a i {
    vertical-align: middle;
}
.tabs-icons>li a:hover {
    color: #067ab4;
}
.tab-pane {
    display: none;
}
.tab-pane.active {
    display: block;
}
.tab-pane.html {
    padding: 20px;
    border: 1px solid #ddd;
    border-top: 10px solid #272822;
    border-radius: 0 0 3px 3px;
}
.att-color-selector-item {
    width: 27px;
    height: 27px;
    border: 2px solid transparent;
    background: 0;
}
.att-color-selector-item.on {
    border-color: #067ab4;
}
.att-color-selector .att-radio-indicator, 
[data-color-selector] .att-radio-indicator {
    width: 21px;
    height: 21px;
    margin: 0;
    border-radius: 21px;
    display: block;
    top: 3px;
    left: 3px;
    opacity: 1;
    -ms-filter: "alpha(opacity=100)"}
.att-accordion {
    border: 1px solid #d3d3d3;
    background: #fff;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}
.att-accordion-group {
    border-top: 1px solid #d3d3d3;
}
.att-accordion-group.first, .att-accordion-group:first-child {
    border-top: 0;
}
.att-accordion-heading a, .att-accordion-inner {
    padding: 10px 20px;
    text-transform: none;
}
.att-accordion-heading a {
    display: block;
    position: relative;
    color: #444;
    font-family: 'clearview_att_regular';
    font-size: 12px;
    font-size: 1.2rem;
    text-transform: uppercase;
}
.att-accordion-heading a:hover {
    color: #199ddf;
}
.att-accordion-heading a:after {
    content: '\e033';
    font-family: atticons;
    position: absolute;
    top: 50%;
    right: 20px;
    font-size: 12px;
    font-size: 1.2rem;
    height: 10px;
    margin-top: -5px;
    -webkit-transition: all .2s ease-out;
    -moz-transition: all .2s ease-out;
    transition: all .2s ease-out;
}
.no-csstransforms .att-accordion-heading a:after {
    -webkit-transition: none;
    -moz-transition: none;
    transition: none;
}
.att-accordion-heading.open a:after {
    -webkit-transform: rotate(-180deg);
    -moz-transform: rotate(-180deg);
    transform: rotate(-180deg);
}
.no-csstransforms .att-accordion-heading.open a:after {
    content: '\e0b5'}
.att-accordion-body {
    height: 0;
    overflow: hidden;
    padding-top: 0;
    -webkit-transition: height .2s ease-out;
    -moz-transition: height .2s ease-out;
    -o-transition: height .2s ease-out;
    transition: height .2s ease-out;
}
.att-accordion-body.open {
    height: auto;
}
.att-accordion-inner {
    padding-top: 0;
}
.att-accordion-inner>* {
    margin: 0;
}
.att-accordion-alt {
    background: 0;
    border: 0;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
}
.att-accordion-alt .att-accordion-group {
    border: 0;
}
.att-accordion-alt .att-accordion-heading a, 
.att-accordion-alt .att-accordion-heading a>*, 
.att-accordion-alt .att-accordion-inner {
    padding: 0 20px;
    text-transform: none;
}
.att-tooltip,
.tooltip {
    background: #ddd;
    border-radius: 3px;
    border: 1px solid #d3d3d3;
    display: block;
    position: absolute;
    z-index: 10000;
    padding: 7px 10px;
    margin: 10px 0 0 0;
    max-width: 215px;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    color: #444;
    font-size: 12px;
    line-height: 15px;
    box-shadow: 0 5px 10px 0 rgba(0, 0, 0, .25);
    opacity: 0;
    -webkit-transition: opacity .2s ease-out;
    -moz-transition: opacity .2s ease-out;
    transition: opacity .2s ease-out;
}
.att-tooltip.on,
.tooltip.in {
    opacity: 1;
}
.att-tooltip:before, .att-tooltip:after,
.tooltip:before, .tooltip:after {
    content: '';
    position: absolute;
    top: -16px;
    left: 50%;
    margin-left: -8px;
    border: 8px solid transparent;
}
.att-tooltip:before,
.tooltip:before {
    top: -17px;
    border-bottom-color: #ccc;
}
.att-tooltip:after,
.tooltip:after {
    border-bottom-color: #ddd;
}
.att-tooltip.right,
.tooltip.right {
    margin: 0 0 0 10px;
}
.att-tooltip.right:before, .att-tooltip.right:after,
.tooltip.right:before, .tooltip.right:after {
    top: 50%;
    left: -8px;
    margin-top: -8px;
    border-bottom-color: transparent;
}
.att-tooltip.right:before,
.tooltip.right:before {
    border-right-color: #ccc;
    left: -9px;
}
.att-tooltip.right:after,
.tooltip.right:after {
    border-right-color: #ddd;
}
.att-tooltip.left,
.tooltip.left {
    margin: 0 0 0 -10px;
}
.att-tooltip.left:before, .att-tooltip.left:after,
.tooltip.left:before, .tooltip.left:after {
    top: 50%;
    left: auto;
    right: -16px;
    margin-top: -8px;
    border-bottom-color: transparent;
}
.att-tooltip.left:before,
.tooltip.left:before {
    border-left-color: #ccc;
    right: -17px;
}
.att-tooltip.left:after,
.tooltip.left:after {
    border-left-color: #ddd;
}
.att-tooltip.above,
.tooltip.top {
    margin: -10px 0 0 0;
}
.att-tooltip.above:before, .att-tooltip.above:after,
.tooltip.top:before, .tooltip.top:after {
    top: auto;
    bottom: -8px;
    margin-top: -8px;
    border-bottom-color: transparent;
    border-bottom-width: 0;
}
.att-tooltip.above:before,
.tooltip.top:before {
    border-top-color: #ccc;
    bottom: -9px;
}
.att-tooltip.above:after,
.tooltip.top:after {
    border-top-color: #ddd;
}
.att-tooltip-light,
.tooltip-light {
    background-color: #fff;
    border-color: #ececec;
}
.att-tooltip-light:before,
.tooltip-light:before {
    border-bottom-color: #ddd;
}
.att-tooltip-light:after,
.tooltip-light:after {
    border-bottom-color: #fff;
}
.att-tooltip-light.right:before {
    border-right-color: #ddd;
}
.att-tooltip-light.right:after {
