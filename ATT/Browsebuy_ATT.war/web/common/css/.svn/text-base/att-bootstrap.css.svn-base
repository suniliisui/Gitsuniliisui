}
.att-tooltip-dark.above:before {
    border-top-color: #111;
}
.att-tooltip-dark.above:after {
    border-top-color: #474747;
}
.att-tooltip-primary {
    background-color: #067ab4;
    border-color: #00689c;
    color: #fff;
}
.att-tooltip-primary:before {
    border-bottom-color: #005783;
}
.att-tooltip-primary:after {
    border-bottom-color: #067ab4;
}
.att-tooltip-primary.right:before {
    border-right-color: #005783;
}
.att-tooltip-primary.right:after {
    border-right-color: #067ab4;
}
.att-tooltip-primary.left:before {
    border-left-color: #005783;
}
.att-tooltip-primary.left:after {
    border-left-color: #067ab4;
}
.att-tooltip-primary.above:before {
